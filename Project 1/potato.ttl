#################################################################
#    Object Properties
#################################################################

###  http://example.com/potato/contains
:contains rdf:type owl:ObjectProperty ;
          owl:inverseOf :isIn ;
          rdf:type owl:TransitiveProperty .


###  http://example.com/potato/costs
:costs rdf:type owl:ObjectProperty ;
       rdfs:domain :Dish .


###  http://example.com/potato/isIn
:isIn rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://example.com/potato#Dairy_product
:Dairy_product rdf:type owl:Class ;
               rdfs:subClassOf :Animal_product .


###  http://example.com/potato/Animal_product
:Animal_product rdf:type owl:Class ;
                rdfs:subClassOf :Ingredient .


###  http://example.com/potato/Dish
:Dish rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :contains ;
                            owl:someValuesFrom :Ingredient
                          ] .


###  http://example.com/potato/Ingredient
:Ingredient rdf:type owl:Class .


###  http://example.com/potato/Macronutrient
:Macronutrient rdf:type owl:Class ;
               rdfs:subClassOf :Nutrient .


###  http://example.com/potato/Meat
:Meat rdf:type owl:Class ;
      rdfs:subClassOf :Animal_product .


###  http://example.com/potato/Micronutrient
:Micronutrient rdf:type owl:Class ;
               rdfs:subClassOf :Nutrient .


###  http://example.com/potato/Nutrient
:Nutrient rdf:type owl:Class .


###  http://example.com/potato/VeganDish
:VeganDish rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:complementOf [ rdf:type owl:Restriction ;
                                                    owl:onProperty :contains ;
                                                    owl:someValuesFrom :Animal_product
                                                  ]
                               ] ;
           rdfs:subClassOf :VegetarianDish .


###  http://example.com/potato/Vegetable
:Vegetable rdf:type owl:Class ;
           rdfs:subClassOf :Ingredient .


###  http://example.com/potato/VegetarianDish
:VegetarianDish rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:complementOf [ rdf:type owl:Restriction ;
                                                         owl:onProperty :contains ;
                                                         owl:someValuesFrom :Meat
                                                       ]
                                    ] ;
                rdfs:subClassOf :Dish .


